<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="users"> 
<!-- com.developer.project.mybatis.UsersMapper => users // UsersDaoImpl에 session.insert("users(a).insert",dto)(a)와 같아야한다.-->
	<insert id="insert" parameterType="com.developer.project.users.dto.UsersDto">
		INSERT INTO users
		(id, pwd, email, regdate)
		VALUES(#{id }, #{pwd }, #{email }, SYSDATE)
	</insert>
</mapper>